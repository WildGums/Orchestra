          <note from="MultilingualBuild" annotates="source" priority="2">{0} = company name
{1} = product name</note>
        </trans-unit>
        <trans-unit id="Orchestra_ThirdPartyNotices_Title" translate="yes" xml:space="preserve">
          <source>Third Party Notices</source>
          <target state="new">Third Party Notices</target>
        </trans-unit>
        <trans-unit id="Orchestra_ThirdPartyNotices" translate="yes" xml:space="preserve">
          <source>Third Party Notices</source>
          <target state="new">Third Party Notices</target>
        </trans-unit>
      </group>
    </body>
